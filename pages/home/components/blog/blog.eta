<section class="cardContainer blogContainer">
  <h2 class="summaryHeading">Blogs</h2>
  <ul class="blogList">
    {{ it.blogs.forEach(blog => { }}
      <li class="blogSummary">
        <h3>{{= blog.title }}</h3>
        <p class="blogInfo">
          <span>{{= blog.readtime }} Minute Readtime</span>
          <span>{{= blog.date }}</span>
          <a href="/view/blog/{{= blog.id }}">Read</a>
        </p>
      </li>
    {{ }) }}
  </ul>
</section>