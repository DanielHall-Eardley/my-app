<li class="ahaMomentContainer">
  <h3 class="ahaMomentTitle">
    <span>{{= it.title }}</span>
    <span>Date: {{= it.date }}</span>
  </h3>
  <p class="ahaMomentRating">
    Rating: 
    {{ for (let i = 0; i <= it.mind_blown_rating; i += 1) { }}
      <span >&#128293;</span>
    {{ } }}
  </p>
  <p>
    {{= it.content }}
  </p>
</li>